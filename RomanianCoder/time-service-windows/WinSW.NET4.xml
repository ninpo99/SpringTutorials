<configuration>
  
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>rc-time-service</id>
  <!-- Display name of the service -->
  <name>RC Time Service (powered by WinSW)</name>
  <!-- Service description -->
  <description>Spring Boot Appliccation running as Windows Service</description>
  
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
  <arguments>-jar %BASE%/rc-time-service.jar</arguments>

</configuration>